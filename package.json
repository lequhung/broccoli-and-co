{
  "name": "broccoli-and-co",
  "version": "0.0.1",
  "description": "Request Invitation Form",
  "main": "src",
  "repository": {
    "type": "git",
    "url": "https://github.com/lequhung/broccoli-and-co.git"
  },
  "author": "Hung Le <hungqule@gmail.com>",
  "homepage": "/",
  "scripts": {
    "build": "npm run clean && webpack --config ./webpack/webpack.config.prod.js",
    "bundle-analyzer": "webpack --config ./webpack/webpack.config.prod.js --profile --json > bundle-analyzer-stats.json",
    "bundle-analyser-open": "webpack-bundle-analyzer bundle-analyzer-stats.json",
    "clean": "rm -rf ./build ./coverage ./cypress-report*.xml ./jest-report.xml",
    "coverage": "npx open-cli ./coverage/lcov-report/index.html",
    "jest-clear-cache": "jest --clearCache",
    "lint": "npm run clean && eslint --fix '**/*.{ts,tsx,js}'",
    "prettier": "npm run clean && prettier --write '**/*.{ts,tsx,js,html,json,css}'",
    "start": "npm run clean && webpack-dev-server --progress --config ./webpack/webpack.config.dev.js",
    "start-test": "npm run clean && webpack-dev-server --config ./webpack/webpack.config.test.js",
    "test": "npm run test:unit && npm run test:functional",
    "test:functional": "concurrently -k -s first \"npm run start-test\" \"npm run wait:localhost-3000 && cypress run --e2e --headless --browser chrome\"",
    "test:functional-open": "concurrently -k -s first \"npm run start\" \"npm run wait:localhost-3000 && cypress open --e2e\"",
    "test:unit": "npm run jest-clear-cache && jest --coverage",
    "test:unit-watch": "npm run jest-clear-cache && jest --watch",
    "test:unit-watch-coverage": "npm run jest-clear-cache && jest --watchAll --coverage",
    "wait:localhost-3000": "wait-on http-get://localhost:3000"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.4.0",
    "core-js": "^3.31.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-emotion": "^10.0.0",
    "react-hook-form": "^7.45.1",
    "react-router-dom": "^6.14.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axe-core": "^4.7.2",
    "concurrently": "^8.2.0",
    "cypress": "^13.4.0",
    "cypress-axe": "^1.4.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.1",
    "jest-cli": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.0",
    "prop-types": "^15.8.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0",
    "webpack-prettier-plugin": "^1.0.3"
  }
}
