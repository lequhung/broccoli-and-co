  "author": "Hung Le <hungqule@gmail.com>",
  "homepage": "/",
  "scripts": {
    "build": "webpack --config ./webpack/webpack.config.prod.js",
    "bundle-analyzer": "webpack --config ./webpack/webpack.config.prod.js --profile --json > bundle-analyzer-stats.json && webpack-bundle-analyzer bundle-analyzer-stats.json",
    "coverage": "open-cli ./coverage/lcov-report/index.html",
    "start": "webpack-dev-server --progress --config ./webpack/webpack.config.dev.js",
    "start-test": "webpack-dev-server --config ./webpack/webpack.config.test.js",
    "test": "npm run test:unit && npm run test:functional",
    "test:functional": "cypress run --headless --browser chrome",
    "test:functional-open": "cypress open",
    "test-unit": "jest --coverage",
    "test-unit:watch": "jest --watch"
  },
